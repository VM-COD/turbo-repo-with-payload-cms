{
  "name": "@repo/ui",
  "version": "0.0.0",
  "private": "true",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "./atoms": {
      "require": "./dist/atoms/index.js",
      "types": "./dist/atoms/index.d.ts",
      "default": "./src/atoms/index.ts"
    },
    "./atoms/*": {
      "require": "./dist/atoms/*/index.js",
      "types": "./dist/atoms/*/index.d.ts",
      "default": "./src/atoms/*/index.ts"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "lint": "eslint src/",
    "dev": "tsup --watch",
    "check-types": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/tailwind-config": "*",
    "@repo/typescript-config": "*",
    "@types/react": "^18.2.61",
    "autoprefixer": "^10.4.18",
    "postcss": "^8.4.39",
    "postcss-modules": "^6.0.0",
    "tailwindcss": "^3.4.6",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@svgr/plugin-jsx": "^8.1.0",
    "esbuild-plugin-svgr": "^2.1.0",
    "esbuild-sass-plugin": "^3.3.1",
    "react-date-picker": "11.0.0",
    "tsup": "^8.2.1"
  }
}
